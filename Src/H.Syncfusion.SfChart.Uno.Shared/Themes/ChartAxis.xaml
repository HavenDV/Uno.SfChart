                                MinWidth="16"
                                Margin="1"
                                Interval="50"
                                IsTabStop="False"
                                Template="{StaticResource HorizontalDecrementTemplate2}"
                                Visibility="{TemplateBinding ScrollButtonVisibility}" />
                            <RepeatButton
                                x:Name="HorizontalLargeDecrease"
                                Grid.Column="1"
                                Width="0"
                                Interval="50"
                                IsTabStop="False"
                                Template="{StaticResource HorizontalRepeatButtonTemplate2}" />
                            <Thumb
                                x:Name="HorizontalThumbHand1"
                                Grid.Column="2"
                                MinWidth="16"
                                Background="{TemplateBinding Background}"
                                Template="{StaticResource HorizontalNearThumbTemplate2}" />
                            <Thumb
                                x:Name="HorizontalThumb"
                                Grid.Column="3"
                                Width="20"
                                MinWidth="0"
                                Background="{TemplateBinding Background}"
                                Template="{StaticResource HorizontalThumbTemplate2}" />
                            <Thumb
                                x:Name="HorizontalThumbHand2"
                                Grid.Column="4"
                                MinWidth="16"
                                Background="{TemplateBinding Background}"
                                Template="{StaticResource HorizontalFarThumbTemplate2}" />
                            <RepeatButton
                                x:Name="HorizontalLargeIncrease"
                                Grid.Column="5"
                                Interval="50"
                                IsTabStop="False"
                                Template="{StaticResource HorizontalRepeatButtonTemplate2}" />
                            <RepeatButton
                                x:Name="HorizontalSmallIncrease"
                                Grid.Column="6"
                                MinWidth="16"
                                Margin="1"
                                Interval="50"
                                IsTabStop="False"
                                Template="{StaticResource HorizontalIncrementTemplate2}"
                                Visibility="{TemplateBinding ScrollButtonVisibility}" />
                        </Grid>

                        <!--  Vertical Template  -->
                        <Grid x:Name="VerticalRoot">
                            <Grid.RowDefinitions>
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="*" />
                                <RowDefinition Height="Auto" />
                            </Grid.RowDefinitions>

                            <!--  Track Layer  -->
                            <Rectangle
                                Grid.RowSpan="7"
                                Fill="#FFB7B7B7"
                                RadiusX="8"
                                RadiusY="8"
                                Stroke="#00000000"
                                StrokeThickness="1" />
                            <Rectangle
                                Grid.RowSpan="7"
                                Fill="#FFB7B7B7"
                                RadiusX="8"
                                RadiusY="8"
                                StrokeThickness="1" />
                            <Rectangle
                                Grid.RowSpan="7"
                                Margin="1"
                                RadiusX="8"
                                RadiusY="8"
                                Stroke="#FFB7B7B7" />

                            <!--  Repeat Buttons + Thumb  -->
                            <RepeatButton
                                x:Name="VerticalSmallDecrease"
                                Grid.Row="6"
                                MinHeight="16"
                                Margin="1"
                                Interval="50"
                                IsTabStop="False"
                                RenderTransformOrigin="0.5,0.5"
                                Template="{StaticResource VerticalDecrementTemplate2}"
                                Visibility="{TemplateBinding ScrollButtonVisibility}">
                                <RepeatButton.RenderTransform>
                                    <RotateTransform Angle="180" />
                                </RepeatButton.RenderTransform>
                            </RepeatButton>
                            <RepeatButton
                                x:Name="VerticalLargeDecrease"
                                Grid.Row="5"
                                Height="0"
                                Interval="50"
                                IsTabStop="False"
                                Template="{StaticResource VerticalRepeatButtonTemplate2}" />
                            <Thumb
                                x:Name="VerticalThumbHand1"
                                Grid.Row="4"
                                MinHeight="18"
                                Template="{StaticResource VerticalNearThumbTemplate2}" />
                            <Thumb
                                x:Name="VerticalThumb"
                                Grid.Row="3"
                                Height="18"
                                MinHeight="0"
                                Template="{StaticResource VerticalThumbTemplate2}" />
                            <Thumb
                                x:Name="VerticalThumbHand2"
                                Grid.Row="2"
                                MinHeight="18"
                                Template="{StaticResource VerticalFarThumbTemplate2}" />
                            <RepeatButton
                                x:Name="VerticalLargeIncrease"
                                Grid.Row="1"
                                Interval="50"
                                IsTabStop="False"
                                Template="{StaticResource VerticalRepeatButtonTemplate2}" />
                            <RepeatButton
                                x:Name="VerticalSmallIncrease"
                                Grid.Row="0"
                                MinHeight="16"
                                Margin="1"
                                Interval="50"
                                IsTabStop="False"
                                RenderTransformOrigin="0.5,0.5"
                                Template="{StaticResource VerticalIncrementTemplate2}"
                                Visibility="{TemplateBinding ScrollButtonVisibility}">
                                <RepeatButton.RenderTransform>
                                    <RotateTransform Angle="180" />
                                </RepeatButton.RenderTransform>
                            </RepeatButton>
                        </Grid>

                        <VisualStateManager.VisualStateGroups>
                            <VisualStateGroup x:Name="CommonStates">
                                <VisualState x:Name="Normal" />
                                <VisualState x:Name="MouseOver" />
                                <VisualState x:Name="Disabled">
                                    <Storyboard>
                                        <DoubleAnimation
                                            Storyboard.TargetName="Root"
                                            Storyboard.TargetProperty="Opacity"
                                            To="0.5"
                                            Duration="0" />
                                    </Storyboard>
                                </VisualState>
                            </VisualStateGroup>
                        </VisualStateManager.VisualStateGroups>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>


