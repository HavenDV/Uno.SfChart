    <Style x:Key="sfResizableBarDesktopStyle" TargetType="local:ResizableScrollBar">
        <Setter Property="MinWidth" Value="15" />
        <Setter Property="MinHeight" Value="15" />
        <Setter Property="IsTabStop" Value="False" />
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="local:ResizableScrollBar">
                    <Grid x:Name="Root">
                        <!--  Horizontal Template  -->
                        <Grid x:Name="HorizontalRoot" Visibility="Collapsed">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="Auto" />
                                <ColumnDefinition Width="Auto" />
                                <ColumnDefinition Width="Auto" />
                                <ColumnDefinition Width="Auto" />
                                <ColumnDefinition Width="Auto" />
                                <ColumnDefinition Width="*" />
                                <ColumnDefinition Width="Auto" />
                            </Grid.ColumnDefinitions>

                            <!--  Track Layer  -->
                            <Rectangle
                                Grid.ColumnSpan="7"
                                Fill="#FFB7B7B7"
                                RadiusX="8"
                                RadiusY="8"
                                Stroke="#00000000"
                                StrokeThickness="1" />
                            <Rectangle
                                Grid.ColumnSpan="7"
                                Fill="{TemplateBinding Background}"
                                RadiusX="8"
                                RadiusY="8"
                                Stroke="#00000000"
                                StrokeThickness="1" />
                            <Rectangle
                                Grid.ColumnSpan="7"
                                Fill="#FFB7B7B7"
                                RadiusX="8"
                                RadiusY="8"
                                StrokeThickness="1" />
                            <Rectangle
                                x:Name="ScrollRect"
                                Grid.ColumnSpan="7"
                                Margin="1"
                                RadiusX="8"
                                RadiusY="8"
                                Stroke="#FFB7B7B7" />

                            <!--  Repeat Buttons + Thumb  -->
