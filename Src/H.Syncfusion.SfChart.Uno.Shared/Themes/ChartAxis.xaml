                Fill="{ThemeResource AppBarBackgroundThemeBrush}"
                Stretch="Uniform"
                Stroke="{ThemeResource ApplicationForegroundThemeBrush}"
                StrokeThickness="3" />
            <Path
                Grid.Row="1"
                Width="10.437"
                Height="6.714"
                Data="M299.906,317.548 L314.401,340.841 L329.346,316.701 C329.346,316.701 316.448,324.029 299.906,317.548 z"
                Fill="{ThemeResource ApplicationForegroundThemeBrush}"
                Stretch="Fill"
                StrokeThickness="6"
